{
  "flex-layout.row#topo-menu": {
    "title": "Topo menu",
    "props": {
      "blockClass": ["topo-menu", "content-custom"],
      "horizontalAlign": "between",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "paddingTop": 2,
      "paddingBottom": 2
    },
    "children": [
      "flex-layout.row#logos-marcas-topo",
      "rich-text#topo-menu-link"
    ]
  },
  "rich-text#topo-menu-link": {
    "title": "Links",
    "props": {
      "text": "[Clube de Águias](https://www.clubedeaguias.com.br) | [Área do Lojista](https://pedidofacil.dakota.com.br/login) | [Atendimento ](/central-de-atendimento) | [Blog](https://blog.dakota.com.br/) | [Sustentabilidade](/no-passo-certo)"
    }
  },
  "flex-layout.row#logos-marcas-topo": {
    "props": {
      "blockClass": "logos-marca",
      "horizontalAlign": "center"
    },
    "children": [
      "image#kolosh",
      "image#missisipi",
      "image#tanara",
      "image#campesi",
      "image#pink_cats",
      "image#kolway"
    ]
  },

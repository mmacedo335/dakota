  "flex-layout.row#topo-flutuante": {
    "title": "Topo Flutuante",
    "props": {
      "blockClass": ["topo-menu-flutuante", "desktop"],
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "verticalAlign": "center",
      "fullWidth": true,
      "paddingTop": 2,
      "paddingBottom": 2
    },
    "children": [
      "rich-text#topo-flutuante-01",
      "rich-text#topo-flutuante-02",
      "rich-text#topo-flutuante-03",
      "rich-text#topo-flutuante-04"
    ]
  },
  "rich-text#topo-flutuante-01": {
    "title": "Conteudo 01",
    "props": {
      "text": "3% de desconto no PIX",
      "textColor": "c-on-base--inverted",
      "blockClass": ["margin-0", "topo-flutuante-01", "topo-flutuante"]
    }
  },
  "rich-text#topo-flutuante-02": {
    "title": "Conteudo 02",
    "props": {
      "text": "Frete grátis para todo Brasil (compras acima de R$ 249,90)",
      "textColor": "c-on-base--inverted",
      "blockClass": ["margin-0", "topo-flutuante-02", "topo-flutuante"]
    }
  },
  "rich-text#topo-flutuante-03": {
    "title": "Conteudo 03",
    "props": {
      "text": "10x sem juros no cartão de crédito",
      "textColor": "c-on-base--inverted",
      "blockClass": ["margin-0", "topo-flutuante-03", "topo-flutuante"]
    }
  },
  "rich-text#topo-flutuante-04": {
    "title": "Conteudo 04",
    "props": {
      "text": "Primeira troca grátis",
      "textColor": "c-on-base--inverted",
      "blockClass": ["margin-0", "topo-flutuante-04", "topo-flutuante"] 
    }
  },
  "flex-layout.row#topo-flutuante-mobile": {
    "title": "Topo Flutuante",
    "props": {
      "blockClass": "topo-menu",
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "verticalAlign": "center",
      "fullWidth": true,
      "paddingTop": 2,
      "paddingBottom": 2
    },
    "children": [
      "slider-layout#topo-flutuante-mobile"
    ]
  },
  "slider-layout#topo-flutuante-mobile": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "autoplay": {
        "timeout": 4000
      },
      "infinite": true,
      "showNavigationArrows": "never",
      "blockClass": "topo-flutuante",
      "showPaginationDots": "never" 
    },
    "children": [
      "rich-text#topo-flutuante-01",
      "rich-text#topo-flutuante-02",
      "rich-text#topo-flutuante-03",
      "rich-text#topo-flutuante-04"
    ]
  }
}
